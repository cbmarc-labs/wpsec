<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	
	<g:HTMLPanel styleName="container">
		
		<g:HTMLPanel ui:field="search" styleName="search"/>
		<g:HTMLPanel ui:field="header" styleName="header"/>
		<g:HTMLPanel ui:field="menu" styleName="menu"/>
		
		<g:HTMLPanel styleName="main">
			<g:HTMLPanel ui:field="content" styleName="content"/>
			<g:HTMLPanel ui:field="sidebar" styleName="sidebar"/>
		</g:HTMLPanel>
		
		<g:HTMLPanel ui:field="footer" styleName="footer"/>
		
	</g:HTMLPanel>
	
</ui:UiBinder> 